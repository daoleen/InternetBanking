<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee
    http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_0.xsd" version="1.0">

    <session name="BankBean">
        <interface binding-name="BankBean" class="com.daoleen.banking.repository.remote.BankRepositoryRemote"/>
    </session>

    <session name="CityBean">
        <interface binding-name="CityBean" class="com.daoleen.banking.repository.remote.CityRepositoryRemote"/>
    </session>

    <session name="ClientAddressBean">
        <interface binding-name="ClientAddressBean"
                   class="com.daoleen.banking.repository.remote.ClientAddressRepositoryRemote"/>
    </session>

    <session name="ClientBean">
        <interface binding-name="ClientBean" class="com.daoleen.banking.repository.remote.ClientRepositoryRemote"/>
    </session>

    <session name="MoneyReservationBean">
        <interface binding-name="MoneyReservationBean"
                   class="com.daoleen.banking.repository.remote.MoneyReservationRepositoryRemote"/>
    </session>

    <session name="PaymentCardBean">
        <interface binding-name="PaymentCardBean"
                   class="com.daoleen.banking.repository.remote.PaymentCardRepositoryRemote"/>
    </session>

    <session name="PaymentTransactionBean">
        <interface binding-name="PaymentTransactionBean"
                   class="com.daoleen.banking.repository.remote.PaymentTransactionRepositoryRemote"/>
    </session>

    <session name="UserBean">
        <interface binding-name="UserBean" class="com.daoleen.banking.repository.remote.UserRepositoryRemote"/>
    </session>

</ejb-jar-bnd>